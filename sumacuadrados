#include "stdafx.h"
#include <iostream>
#include "conio.h"
#include "math.h"

using namespace std;
void main ()
{
	int s,i,n
	cout<<"Introduzca el valor de n: ";
	cin>>n;
	s=0;
	for (i=1;i<=n;i++)
		s=s+pow(i,2);
	cout<<s;
	getch();
}
