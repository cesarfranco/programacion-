#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

int main() {
	float base, altura, area;
	cout<<"Ingrese la altura: ";
	cin>>altura;
	cout<<"Ingrese la base: ";
	cin>>base;
	area = (base*altura);
	cout<<"El area del rectangulo es: ";
	cout<<area;
	getch();
}

